{
    "name": "fastify-gql",
    "version": "1.0.0",
    "description": "Template to quickstart API development with Fastify and GraphQL",
    "main": "dist/index.js",
    "scripts": {
        "dev": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.test.ts' --exec 'ts-node --pretty --files' src/index.ts",
        "start": "node dist/index.js",
        "test": "mocha",
        "build": "rm -rf dist && tsc -b --listEmittedFiles",
        "debug": "node --inspect -r ts-node/register src/index.ts",
        "start:prisma": "docker-compose up -d",
        "deploy:models": "prisma deploy"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dannysofftie/fastify-gql.git"
    },
    "keywords": [
        "fastify",
        "fastify-graphql",
        "fastify-graphql-api"
    ],
    "author": "Danny Sofftie",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/dannysofftie/fastify-gql/issues"
    },
    "homepage": "https://github.com/dannysofftie/fastify-gql#readme",
    "dependencies": {
        "@prisma/client": "^2.0.0-beta.1",
        "aedes": "^0.40.1",
        "aedes-persistence": "^8.1.1",
        "apollo-server-fastify": "^2.13.0",
        "bcrypt": "^5.0.0",
        "dotenv": "^8.2.0",
        "fastify": "^2.14.1",
        "fastify-cors": "^3.0.1",
        "fastify-plugin": "^1.6.1",
        "graphql": "^14.6.0",
        "graphql-middleware": "^4.0.2",
        "graphql-shield": "^7.2.6",
        "graphql-tools": "^4.0.8",
        "html-minifier": "^4.0.0",
        "jsonwebtoken": "^8.5.1",
        "nodemailer": "^6.4.6",
        "serve-static": "^1.14.1",
        "sqlite3": "^4.2.0",
        "websocket-stream": "^5.5.2"
    },
    "engines": {
        "node": ">=12.0.0"
    },
    "devDependencies": {
        "@prisma/cli": "^2.0.0-beta.1",
        "@types/html-minifier": "^3.5.3",
        "@types/jsonwebtoken": "^8.3.7",
        "@types/node": "^13.13.5",
        "@types/nodemailer": "^6.4.0",
        "nodemon": "^2.0.3",
        "ts-node": "^8.10.1",
        "typescript": "^3.8.3"
    }
}
