{
    "name": "fastify-gql",
    "version": "1.0.0",
    "description": "Template to quickstart API development with Fastify and GraphQL",
    "main": "dist/index.js",
    "scripts": {
        "dev": "ts-node-dev --respawn --no-notify --files --ignore-watch node_modules src/index.ts",
        "start": "node dist/index.js",
        "test": "test",
        "build": "npm run clean && tsc -b --listEmittedFiles",
        "clean": "tsc -b --clean",
        "debug": "node --inspect -r ts-node/register src/index.ts",
        "start:prisma": "docker-compose up -d",
        "deploy:models": "prisma deploy"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dannysofftie/fastify-gql.git"
    },
    "keywords": [
        "fastify",
        "fastify-graphql",
        "fastify-graphql-api"
    ],
    "author": "Danny Sofftie",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/dannysofftie/fastify-gql/issues"
    },
    "homepage": "https://github.com/dannysofftie/fastify-gql#readme",
    "dependencies": {
        "apollo-server-fastify": "^2.9.16",
        "fastify": "^2.11.0",
        "fastify-cors": "^3.0.1",
        "graphql": "^14.6.0",
        "graphql-tools": "^4.0.6",
        "prisma-client-lib": "^1.34.10",
        "serve-static": "^1.14.1"
    },
    "engines": {
        "node": ">=12.0.0"
    },
    "devDependencies": {
        "@types/node": "^13.7.0",
        "jest": "^25.1.0",
        "ts-jest": "^25.2.0",
        "ts-node-dev": "^1.0.0-pre.44",
        "typescript": "^3.7.5"
    }
}
